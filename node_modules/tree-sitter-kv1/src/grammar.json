      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "true"
        },
        {
          "type": "PATTERN",
          "value": "false"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\\\"[^\\\"]*\\\""
    },
    "number": {
      "type": "PATTERN",
      "value": "[+-]?([0-9]*[.])?[0-9]+"
    }
